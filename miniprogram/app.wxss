/**app.wxss**/
@import "weui.wxss";

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

.options {
  width: 94%;
  border-radius: 10rpx;
  background-color: #fff;
  border: 1rpx solid #E3E3E3;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.weui-cell:before{
  left:30rpx;
  right:30rpx;
}
